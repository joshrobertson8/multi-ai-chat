{
  "name": "multi-ai-chat",
  "version": "1.0.0",
  "description": "Responsive web chat app that lets users talk to multiple free AI models",
  "main": "backend/server.js",
  "scripts": {
    "install-all": "npm install --prefix backend && npm install --prefix frontend",
    "dev": "concurrently \"npm run dev --prefix backend\" \"npm run dev --prefix frontend\"",
    "start": "npm start --prefix backend",
    "build": "npm run build --prefix frontend",
    "test": "echo \"No tests specified yet\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshrobertson8/multi-ai-chat.git"
  },
  "keywords": [
    "ai",
    "chat",
    "openai",
    "gemini",
    "huggingface",
    "mistral",
    "react",
    "typescript",
    "nodejs"
  ],
  "author": "Josh Robertson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshrobertson8/multi-ai-chat/issues"
  },
  "homepage": "https://github.com/joshrobertson8/multi-ai-chat#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}